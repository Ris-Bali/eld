#---InputFilePluginAPI.test----------------------- Executable,LS ----------------#
#BEGIN_COMMENT
# Tests the inputfile plugin APIs for both objectfiles and
# archive member files.
#END_COMMENT
#START_TEST
RUN: %clang %clangopts -c %p/Inputs/1.c -o %t1.1.o -ffunction-sections
RUN: %clang %clangopts -c %p/Inputs/2.c -o %t1.2.o -ffunction-sections
RUN: %ar cr %aropts %t1.lib2.a %t1.2.o
RUN: %link %linkopts %t1.1.o %t1.lib2.a -o %t2.out --plugin-config %p/Inputs/plugin.config -L %p/Inputs 2>&1 | %filecheck %s

#CHECK: The hash of {{.*}}1.o is: {{[0-9]+}}
#CHECK: The hash of archive member {{.*}}2.o is: {{[0-9]+}}